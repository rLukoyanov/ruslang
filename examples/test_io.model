% Тест операций ввода-вывода %
begin
    var x, y, z: int;
    var name: int;
    
    % Ввод одной переменной %
    readln x;
    
    % Ввод нескольких переменных %
    readln y, z;
    
    % Вывод одной переменной %
    writeln x;
    
    % Вывод нескольких переменных %
    writeln x, y, z;
    
    % Вывод выражения %
    writeln x plus y
end
