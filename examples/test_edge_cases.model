% Тест граничных случаев %
begin
    var x: int;
    var f: float;
    var b: bool;
    
    % Минимальное значение %
    x := 0;
    writeln x;
    
    % Пустой составной оператор %
    if (x EQ 0)
        writeln x;
    
    % Множественные пробелы и переносы %
    x    :=    10;
    
    
    writeln    x;
    
    % Логические константы %
    b := true;
    b := false;
    writeln b;
    
    % Отрицательные числа %
    x := 5;
    x := 0 min x;
    writeln x;
    
    % Цепочка присваиваний %
    f := 1.0;
    writeln f
end
